<!DOCTYPE html>
<html>
<head>
  <title>Add commute photo</title>
  {{> headers-common }}
</head>
<body>
  {{> header }}

  <h1>Add commute photo</h1>
  <div>
    <div class="profilesection">
      <form class="userForm" id="registerform" action="addcommutephoto.php" method="post" enctype="multipart/form-data">
          <input type="file" name="photo" id="photo" /><span>or <a href="#">delete photo</a></span><br />
          <input type="text" name="commuteId" value="{{commuteId}}" />
          <button type="submit" name="button">Save</button>
      </form>
    </div>
  </div>

  {{> footer }}
</body>
</html>
